{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@collection": ["mfes/editors/src/components/QuestionSetEditor.tsx"],
      "@content/*": ["mfes/*"],
      "@login": ["mfes/authentication/src/pages/login.tsx"],
      "@mfe-next": ["mfes/mfe-next/src/app/page.tsx"],
      "@mfe-next-2": ["mfes/mfe-next-2/src/app/page.tsx"],
      "@mfe-next-page-1": ["mfes/mfe-next-page-1/src/pages/_app.tsx"],
      "@mfe-next-page-2": ["mfes/mfe-next-page-2/src/pages/_app.tsx"],
      "@players": ["mfes/players/src/pages/play.tsx"],
      "@questionSet": ["mfes/editors/src/components/CollectionEditor.tsx"],
      "@registration": ["mfes/registration/src/app/newUser/page.tsx"],
      "@scp-teacher": ["mfes/scp-teacher/src/pages/index.tsx"],
      // "@shared-lib": ["libs/shared-lib/src/index.ts"],
      "@shared-lib/*": ["libs/shared-lib/src/*"],
      "@shikshagrah/ui-theme": ["shared/src/index.ts"],
      "@shikshav2.0/shared": ["libs/shared/src/index.ts"],
      "@test-next-route": ["mfes/test-next-route/src/pages/index.tsx"],
      "@test-react-vite": ["mfes/mfes/test-react-vite"],
      "@youthNet": ["mfes/youthNet/src/pages/index.tsx"],
      "@content-mfes/*": ["mfes/content/src/*"],

      "@Content": ["mfes/content/src/components/Content/List.tsx"],
      "@ContentEnrollDetails": [
        "mfes/content/src/components/Content/ContentEnroll.tsx"
      ],
      "@CourseUnitDetails": [
        "mfes/content/src/components/Content/CourseUnitDetails.tsx"
      ],
      "@InfoCard": ["mfes/content/src/components/Card/InfoCard.tsx"],
      "@ContentPlayer": ["mfes/content/src/components/Content/Player.tsx"],

      //dynamic form
      "@shared-lib": ["libs/shared-lib-v2/src/index.tsx"],
      "@shared-lib-v2/*": ["libs/shared-lib-v2/src/*"],
      "@players/*": ["mfes/players/src/pages/play.tsx"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
